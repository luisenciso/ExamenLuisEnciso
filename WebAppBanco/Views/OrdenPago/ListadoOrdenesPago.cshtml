@model List<Datos.Models.OrdenPago>
@{
    ViewData["Title"] = "ListadoOrdenesPago";
    Layout = "~/Views/Shared/_LayoutBanco.cshtml";
}

<h2>Listado de Ordenes de Pago</h2>

<table class="table table-hover">
    <thead>
        <tr>
            <td>Monto</td>
            <td>Fecha Registro</td>
            <td>Moneda</td>
            <td>Estado</td>
            <td>Sucursal</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelitem => item.Monto)</td>
                <td>@Html.DisplayFor(modelitem => item.FechaRegistro)</td>
                <td>@Html.DisplayFor(modelitem => item.Moneda)</td>
                <td>@Html.DisplayFor(modelitem => item.Estado)</td>
                <td>@Html.DisplayFor(modelitem => item.sucursal)</td>
            </tr>
        }
    </tbody>
</table>

